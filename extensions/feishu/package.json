{
  "name": "@moltbot/feishu",
  "version": "2026.1.26",
  "description": "OpenClaw Feishu/Lark channel plugin",
  "type": "module",
  "dependencies": {
    "@larksuiteoapi/node-sdk": "^1.36.0",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "openclaw": "workspace:*"
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.26"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "feishu",
      "label": "Feishu",
      "selectionLabel": "Feishu (飞书)",
      "docsPath": "/channels/feishu",
      "docsLabel": "feishu",
      "blurb": "飞书/Lark messaging; WebSocket or Webhook.",
      "aliases": [
        "lark"
      ],
      "order": 65
    },
    "install": {
      "npmSpec": "@moltbot/feishu",
      "localPath": "extensions/feishu",
      "defaultChoice": "npm"
    }
  }
}
